{
  "name": "vue-date-input",
  "version": "1.0.0",
  "description": "Fully accessible date range input/picker",
  "repository": "https://gitlab.com/jinusean/vue-date-input",
  "author": "James C. Lee",
  "license": "MIT",
  "private": false,
  "main": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "unpkg": "dist/index.min.js",
  "browser": {
    "./sfc": "src/index.js"
  },
  "scripts": {
    "dev": "vue serve --open example/App.vue",
    "build": "rm -rf dist && npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/my-component.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/my-component.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/my-component.min.js",
    "lint": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "slint": "stylelint 'src/styles' --fix --formatter verbose"
  },
  "devDependencies": {
    "autoprefixer": "^9.4.3",
    "babel-eslint": "^10.0.1",
    "cssnano": "^4.1.8",
    "eslint": "5.10.0",
    "eslint-plugin-vue": "5.0.0",
    "node-sass": "^4.11.0",
    "postcss": "^7.0.7",
    "prettier": "^1.15.3",
    "rollup": "^0.68.0",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-scss": "^0.4.0",
    "rollup-plugin-svg": "^1.0.1",
    "rollup-plugin-vue": "^4.3.2",
    "sass-loader": "^7.1.0",
    "stylelint": "^9.9.0",
    "stylelint-config-recess-order": "^2.0.1",
    "stylelint-prettier": "^1.0.5",
    "vue": "^2.5.21",
    "vue-template-compiler": "^2.5.21"
  },
  "dependencies": {
    "element-ui": "^2.4.11",
    "tabbable": "3.1.1"
  }
}
